{
    "version": 2,
    "builds": [
        {
            "src": "luzysonido_v2/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb" ,"runtime":"python3.9"}
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "luzysonido_v2/wsgi.py"
        }
    ]
}